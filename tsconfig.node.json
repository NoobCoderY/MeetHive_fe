{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "noEmit": false,
    // ...
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@shadcn/*": ["./src/shadcn/*"]
    }
    // ...
  },
  "include": ["vite.config.ts"]
}
